<h1>Smaug Lavaflow</h1>
<ul class="list-unstyled">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.username, user_path(user) %>
    </li>
  <% end %>
</ul>
<%= link_to "Login", login_path, class: "btn btn-success" %>
<%= link_to "Add user", new_user_path, class: "btn btn-info" %>
<%= link_to "Logout", logout_path, class: "btn btn-danger" %>
