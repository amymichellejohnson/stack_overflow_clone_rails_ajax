<%= bootstrap_form_for @user do |f| %>
  <div class="form-group">
    <%= f.text_field :username %>
  </div>

  <div class="form-group">
    <%= f.password_field :password %>
  </div>

  <div class="form-group">
    <%= f.password_field :password_confirmation %>
  </div>

  <%= f.submit 'Create User', class: "btn btn-success" %>
<% end %>
